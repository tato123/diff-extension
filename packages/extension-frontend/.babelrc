{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 3 Chrome versions"]
        },
        "modules": false
      }
    ],
    "@babel/typescript",
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
        "@babel/typescript",
        ["@babel/env", { "modules": "commonjs" }]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  }
}
