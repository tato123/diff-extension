{
  "rules": [
    {
      "pathname": "/(static|socket.io|signup|sockjs-node)",
      "dest": "http://localhost:3000"
    },
    {
      "pathname": "/(static|socket.io|signup|sockjs-node)/**",
      "dest": "http://localhost:3000"
    },
    { "pathname": "/", "dest": "http://localhost:8000" }
  ]
}
