{
  "presets": [
    "@babel/preset-react",
    "@babel/typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 Chrome versions"]
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-lodash",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
        "@babel/typescript",
        ["@babel/env", { "modules": "commonjs" }]
      ],
      "plugins": ["@babel/plugin-proposal-class-properties"]
    }
  }
}
